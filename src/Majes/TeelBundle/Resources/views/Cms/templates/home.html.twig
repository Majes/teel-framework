{% extends 'MajesTeelBundle::layout.html.twig' %}
{% block title %}{{parent()}} New symfony2 framework{% endblock %}

{% block meta_type %}{{parent()}}{% endblock %}
{% block meta_title %}{{parent()}}{% endblock %}
{% block meta_description %}{% endblock %}
{% block meta_image %}{% endblock %}


{% block content %}

{{parent()}}
<!-- Start of Page Container -->
<div class="page-container">
        <div class="container">
                <div class="row">
                        <!-- start of page content -->
                        <div class="span8 page-content">

                            {{ render_esi(controller('MajesCmsBundle:Index:block', { 'block' : content.home_functionalities })) }}

                        </div>
                        <!-- end of page content -->


                        <!-- start of sidebar -->
                        <aside class="span4 page-sidebar">

                                <section class="widget">
                                        <div class="support-widget">
                                            {{ render_esi(controller('MajesCmsBundle:Index:block', { 'block' : content.home_support })) }}
                                        </div>
                                </section>

                                <section class="widget">
                                        <div class="quick-links-widget">
                                            {{ render_esi(controller('MajesCmsBundle:Index:block', { 'block' : content.home_links })) }}
                                        </div>
                                </section>

                        </aside>
                        <!-- end of sidebar -->
                </div>
        </div>
</div>
<!-- End of Page Container -->

{% endblock %}

